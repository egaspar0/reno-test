{
  "extends": [ "config:best-practices"],
  "ignorePresets": ["group:monorepos"],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"]
    }
  ],
  "transitiveRemediation" : true,
  "vulnerabilityAlerts": {
    "groupName": "{{{vulnerabilitySeverity}}} vulnerability alerts",
    "enabled": true
  },
  "osvVulnerabilityAlerts": true
}
